{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:node/recommended",
    "eslint:recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": "off",
    "consistent-return": "off",
    "node/no-unsupported-features/es-syntax": "off",
    "import/prefer-default-export": "off",
    "node/no-unpublished-require": "off"
  }
}
