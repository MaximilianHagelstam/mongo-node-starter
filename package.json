{
  "name": "create-simple-express-api",
  "version": "1.0.0",
  "description": "Setup a modern API by running one command",
  "bin": ".bin/cli.js",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaximilianHagelstam/create-simple-express-api.git"
  },
  "bugs": {
    "url": "https://github.com/MaximilianHagelstam/create-simple-express-api/issues"
  },
  "author": "Maximilian Hagelstam",
  "license": "MIT",
  "keywords": [
    "express",
    "node"
  ],
  "type": "module",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1"
  }
}
